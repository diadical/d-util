{
  "name": "d-util",
  "version": "0.0.0",
  "description": "Data structures and algorithms.",
  "main": "dist/d-util.js",
  "scripts": {
    "clean": "rm -rf dist tmp",
    "build": "rm -rf dist && ./node_modules/.bin/broccoli build dist",
    "watch": "./node_modules/.bin/broccoli-timepiece dist",
    "lint": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/mocha",
    "docs": "./node_modules/.bin/jsdoc -d docs/ -r src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diadical/d-util.git"
  },
  "keywords": [
    "javascript",
    "data structures",
    "algorithms",
    "library",
    "computer science"
  ],
  "author": "Diwas Adhikary",
  "license": "MIT",
  "homepage": "https://github.com/diadical/d-util",
  "bugs": {
    "url": "https://github.com/diadical/d-util/issues"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "broccoli": "^0.16.8",
    "broccoli-babel-transpiler": "^5.4.5",
    "broccoli-cli": "^1.0.0",
    "broccoli-concat": "0.0.13",
    "broccoli-funnel": "^0.2.8",
    "broccoli-merge-trees": "^0.2.3",
    "broccoli-timepiece": "^0.3.0",
    "chai": "^3.2.0",
    "eslint": "^1.3.1",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "mocha": "^2.3.0",
    "mocha-eslint": "^1.0.0"
  }
}
